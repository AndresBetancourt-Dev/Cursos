{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[javascript,javascriptreact,typescript,typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "editor.tabSize": 2,
  "files.eol": "\n",
  "prettier.bracketSpacing": false
}
